<view class="page">
    <view class="page__bd">
        <view class="weui-cells__title">播放列表</view>
        <view class="weui-cells weui-cells_after-title">
            <navigator wx:for="{{musicList}}" wx:key="{{item._id}}" url="/pages/detail/detail?mid={{item._id}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd">{{item.name}} - {{item.author}}</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>            
        </view>
    </view>
</view>